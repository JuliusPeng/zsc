{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint-all": "eslint ./ --ext .json,.js",
    "lint-dashboard": "eslint ./projs/dashboard --ext .json,.js",
    "node": "cross-env NODE_ENV=development DEBUG=backend:* node ./server.js",
    "babel": "set DEBUG=backend:* & babel-node ./server.js",
    "pm2": "set DEBUG=backend:* & pm2 start ./server.js --name dashboard --watch",
    "pm2-all": "set DEBUG=backend:* & pm2 start ./server.js --name dashboard --watch -i max",
    "pm2-all-build": "set NODE_ENV=production & pm2 start ./server.js --name dashboard --watch -i max"
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "http-errors": "^1.7.3",
    "jsonwebtoken": "^8.5.1",
    "jssha": "^2.3.1",
    "log4js": "^4.4.0",
    "moment": "^2.24.0",
    "mongoose": "^5.6.5",
    "morgan": "^1.9.1",
    "nodemailer": "^6.3.0",
    "saslprep": "^1.0.3",
    "thirty-two": "^1.0.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "config": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-config-elemefe": "^0.3.0",
    "eslint-plugin-json": "^2.0.1"
  }
}
